{{#*inline "page"}}
  <ol>
  {{#each lectures}}
  {{#each this.questions}}
    <li>
        <h5>Edit Lecture {{{ this.lid }}} Question {{{ this.qid }}}:</h5>
        <form action="/edit/{{{ this.lid }}}/{{{ this.qid }}}" method="post" accept-charset="utf-8">
            <label>Your Decryption Capability:
              <p>
              <input name="decryption_cap" />
              </p>
            </label>
            <input type=hidden name="ownership_loc_cap" value="{{{ this.olc }}}"/>
            <input type="submit" value="Submit">
        </form>
    </li>
  {{/each}}
  </ol>
{{/inline}}
{{~> (parent)~}}
